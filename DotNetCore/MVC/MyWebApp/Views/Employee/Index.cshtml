@model List<MyWebApp.Models.Employee>

@{
    ViewData["Title"] = "Index";
}

<html>
<head>
    <title>Employee Table</title>
</head>
</br>
<span class="text-success"> <b> @ViewBag.SuccessMessageForAdd </b> </span>
<br />
<div>
    <a href="http://localhost:5277/Employee/Add"> <b>Add</b> </a>
</div>
<br />
<body>
    <form>
        <table border="3px" class="table">
            <tr class="table1">
                <td><b>Full Name</b></td>
                <td><b>Father's Name</b></td>
                <td><b>Email</b></td>
                <td><b>CountryId</b></td>
            </tr>

            @foreach (Employee employee in Model)
            {
                <tr class="table1">
                    <td><b>@employee.FullName</b></td>
                    <td><b>@employee.FatherName</b></td>
                    <td><b>@employee.Email</b></td>
                    <td><b>@employee.CountryId</b></td>
                </tr>
            }
        </table>
    </form>
</body>
</html>
