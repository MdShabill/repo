<div class="col-md-4">
    <div id="dataContainer">
    </div>
    <div style="margin-left: 2px;">
        <input type="submit" id="AddToCart" value="Add To Cart" class="btn btn-primary" />
    </div>
</div>

<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
<script type="text/javascript">
    $(function () {
        function performAction(buttonId) {
            $.ajax({
                type: "GET",
                headers: {
                    'Access-Control-Allow-Origin': '*',
                }
                url: "https://qa-member.astm.org/m1c/api/v1/value",
                // url: "https://localhost:7073/api/Doctors/GetAllDoctors",
                success: function (response) {

                    debugger;

                    $("#dataContainer").empty();
                    response.forEach(function (item) {
                        $("#dataContainer").append("<p>Id: " + item.id + "</p>");
                        $("#dataContainer").append("<p>FullName: " + item.fullName + "</p>");
                        $("#dataContainer").append("<p>Gender: " + item.gender + "</p>");
                        $("#dataContainer").append("<p>Email: " + item.email + "</p>");
                        $("#dataContainer").append("<p>RegistrationNumber: " + item.registrationNumber + "</p>");
                        $("#dataContainer").append("<p>Department: " + item.department + "</p>");
                        $("#dataContainer").append("<p>City: " + item.city + "</p>");
                    });
                },
                failure: function (response) {
                    debugger;
                    alter(response);
                },
                error: function (response) {
                    debugger;
                    alter(response);
                }
            });
        }
        $("#AddToCart").click(function () {
            performAction("AddToCart");
        });
    });
</script>